<div id="pageTitle">
	<h1><%= @account.id%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @account.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @account.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @account.Name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Type</td>
        <td class="itemValue"><%= @account.Type%></td>
      </tr>
      
    </table>
</div>