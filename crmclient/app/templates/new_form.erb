<div class="pageTitle">
  <h1><%= @doc_def["title"] %></h1>
</div>

<div class="toolbar">
  <div class="leftItem backButton">
    	<a class="cancel" href="<%= @doc_def["object"] %>/index">Cancel</a>
  </div>
</div>

<div class="content">
  <form method="POST" action="<%= @doc_def['object'] %>/create">
    <input type="hidden" name="id" value="@<%=@doc_def["model"]%>.object %>"/>
    
	<%@content.each_line do |line|%><%= "  " + line%><%end%>

    <input type="submit" class="standardButton" value="Create"/>
  </form>
</div>
